<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.notiufg"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_ufg_branco"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.notiufg.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.notiufg.activity.LoginActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.notiufg.activity.NovoUsuarioActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.notiufg.activity.MenuActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.notiufg.activity.ConfiguracaoActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.notiufg.activity.ListNotificacaoActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.notiufg.NotificationActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.notiufg.activity.SingleNotificationActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>
